{
  "name": "lets-rest",
  "version": "1.0.0",
  "description": "Boilerplate for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-2\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-2",
    "start": "NODE_ENV=production node dist",
    "prestart": "npm run -s build",
    "test": "mocha --timeout 10000 --compilers js:babel-register",
    "coverage": "nyc --temp-directory ./test/coverage/tmp mocha --timeout 10000 --compilers js:babel-register",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ollelauribostrom/lets-rest.git"
  },
  "keywords": [
    "es6",
    "rest",
    "express",
    "babel",
    "mocha",
    "chai",
    "nyc"
  ],
  "author": "Olle Lauri Bostr√∂m (https://github.com/ollelauribostrom)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-rate-limit": "^2.8.0",
    "helmet": "^3.6.1",
    "morgan": "^1.8.0",
    "rotating-file-stream": "^1.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2",
    "nyc": "^11.0.3-candidate.0"
  },
  "nyc": {
    "report-dir": "test/coverage",
    "cache": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "bugs": {
    "url": "https://github.com/ollelauribostrom/lets-rest/issues"
  },
  "homepage": "https://github.com/ollelauribostrom/lets-rest#readme"
}
